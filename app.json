{
  "expo": {
    "name": "deniz-market",
    "slug": "deniz-market",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "denizmarket",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "buildConfiguration": "Release",
      "autoIncrement": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.deniz.market",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "buildType": "app-bundle",
      "autoIncrement": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#4A2C2A"
      },
      "versionCode": 1,
      "package": "com.deniz.market",
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.491979314052-k2kna9glv0phkhbjf59tiaikfmugd2nu"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "702cb85e-82a8-4527-a574-975ecb990550"
      }
    }
  }
}